<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cupk.mapper.PlateMapper">
  <select id="findAllPlate" resultType="com.cupk.pojo.PlateMessage">
    select * from t_plate
  </select>
  <select id="findPlateById" resultType="com.cupk.pojo.PlateMessage">
    select * from t_plate where id=#{id}
  </select>
</mapper>